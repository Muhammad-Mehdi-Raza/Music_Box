@{
    Layout = null;
}

<!DOCTYPE html>


<html class="no-js">


<head>
    <title>Home - Music Box</title>
    <meta charset="utf-8">

    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/main.css" rel="stylesheet" />
    <link href="~/Content/animations.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/fonts.css" rel="stylesheet" />
    <link href="~/Content/index.css" rel="stylesheet" />

    <script src="~/Scripts/modernizr-2.6.2.min.js"></script>

</head>
<body>

    <div id="box_wrapper">

        <div id="logo-header">
            <div class="container">
                <div class="logo-container clearfix">
                    <div class="logo pull-left">
                        <a class="navbar-brand" href="../Client/Index"> <img src="~/Images/logo.png" alt=""></a>
                    </div>

                    <div class="social-info pull-right">
                        <ul class="social">
                            <li class="phone">
                                <a href="#">
                                    <i class="fa fa-phone"></i>
                                    <span>0092-311-0143743</span>
                                </a>
                            </li>
                            <li class="mail active">
                                <a href="#">
                                    <i class="fa fa-envelope"></i>
                                    <span>bsef12m543@pucit.edu.pk</span>
                                </a>
                            </li>
                            <li class="facebook">
                                <a href="#">
                                    <i class="fa fa-facebook"></i>
                                    <span>follow on facebook</span>
                                </a>
                            </li>
                            <li class="icon_twitter">
                                <a href="#">
                                    <i class="fa fa-twitter"></i>
                                    <span>follow us on Twitter</span>
                                </a>
                            </li>
                            <li class="pinterest">
                                <a href="#">
                                    <i class="fa fa-pinterest"></i>
                                    <span>get pin on pinterest</span>
                                </a>
                            </li>
                            <li class="googleplus">
                                <a href="#">
                                    <i class="fa fa-google-plus"></i>
                                    <span>follow on google plus</span>
                                </a>
                            </li>
                            <li class="rss">
                                <a href="#">
                                    <i class="fa fa-rss"></i>
                                    <span>subscrive via rss</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>

        <header id="header">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12 grey_section">
                        <span id="toggle_mobile_menu"></span>
                        <nav id="mainmenu_wrapper">
                            <ul id="mainmenu" class="nav sf-menu">
                                <li class="">
                                    <a href="#logo-header">Home</a>
                                </li>
                                <li class="">
                                    <a href="#events">About Us</a>
                                </li>
                                <li class="">
                                    <a href="~/Client/Songs">Manage Songs</a>
                                </li>
                                <li class="">
                                    <a href="~/Song/Explore">Explore</a>
                                </li>
                                <li class="">
                                    <a href="#play_list">PlayList</a>
                                </li>
                                <li class="">
                                    <a href="#portfolio">Projects</a>
                                </li>
                                <li class="">
                                    <a href="~/Client/Favourites">Favourites</a>
                                </li>
                                <li class="">
                                    <a href="#contact">Contact</a>
                                </li>
                                <li class="">
                                    @Html.ActionLink("Logout", "Logout", "Account")
                                </li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </header>

        @RenderBody()
        @RenderSection("Scripts", required: false)
    </div>

    <div id="copyright" class="darkgrey_section">
        <div class="container">
            <div class="row">
                <div class="col-sm-6">
                    <!--                    <p>&copy; Copyright 2015 - Music Box </p>  -->
                </div>
                <div class="col-sm-6">
                    <div class="widget widget_nav_menu">

                        <div>
                            <ul class="menu">
                                <li class="">
                                    <a href="https://www.facebook.com/Rkr.Einstein">Powered by Einstein</a>
                                </li>
                                <li>
                                    <p>&copy; Copyright 2015 - Music Box </p>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>

</body>
</html>

