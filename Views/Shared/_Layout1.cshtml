        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>


<body style="background: url('../../Images/Dark.png') center">

    <nav class="navbar navbar-inverse" role="navigation">
        <div class="navbar-header">
            <a class="navbar-brand" href="#"><p style="font-family:Chiller; font-size:xx-large; font-weight:900; margin-top:3px">Admin Panel - Music Box</p></a>
        </div>
        <div>
            <ul class="nav navbar-nav pull-right">
                <li>@Html.ActionLink("Home", "Index", "Admin")</li>
                <li>@Html.ActionLink("Logout", "Logout", "Account")</li>
            </ul>
        </div>
    </nav>

    @RenderBody()

